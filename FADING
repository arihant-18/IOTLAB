int pin=9;
void setup() {
  pinMode(pin,OUTPUT);
  
  // put your setup code here, to run once:

}

void loop() {
  int i;
  for(i=0;i<255;i++) {
    analogWrite(pin,i);
    delay(30);
  }
  for(i=255;i>0;i--) {
    analogWrite(pin, i);
    delay(30);
  }
  // put your main code here, to run repeatedly:

}
